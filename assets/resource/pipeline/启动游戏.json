{
    "BD2启动": {
        "action": "StartApp",
        "package": "com.neowizgames.game.browndust2",
        //30秒后执行"检查启动页面"
        "post_delay": 30000,
        "next": [
            "检查启动页面"
        ]
    },
    "检查启动页面": {
        "timeout": 600000,
        "rate_limit": 10000,
        "recognition": "TemplateMatch",
        "template": "启动页面设置.png",
        "green_mask": true,
        "next": [
            "下载资源",
            "登录失败",
            "进入游戏"
        ]
    },
    "下载资源": {
        "recognition": "TemplateMatch",
        "template": "下载资源.png",
        "action": "Click",
        "next": [
            "检查启动页面"
        ]
    },
    "登录失败": {
        "recognition": "OCR",
        "expected": [
            "登录失败",
            "连线请求超时",
            "支付失败"
        ],
        "action": "Click",
        "pre_delay": 1000,
        "target": [
            1000,
            601,
            1,
            1
        ],
        "next": [
            "检查启动页面"
        ]
    },
    "进入游戏": {
        "recognition": "TemplateMatch",
        "template": "进入游戏.png",
        "action": "Click",
        //5秒后执行"回到主菜单"
        "post_delay": 10000,
        "next": [
            "确认收菜",
            "回到主菜单"
        ]
    },
    "回到主菜单": {
        "recognition": "TemplateMatch",
        "green_mask": true,
        "roi": [
            87,
            154,
            64,
            56
        ],
        "template": "副本.png",
        "target": [
            1182,
            47,
            1,
            1
        ],
        "pre_delay": 1000,
        "action": "Click",
        "post_delay": 5000
    },
    "确认收菜": {
        "timeout": 5000,
        "recognition": "TemplateMatch",
        "template": "每日感叹号.png",
        "green_mask": true,
        "roi": [
            826,
            516,
            105,
            100
        ],
        "pre_delay": 4000,
        "action": "Click",
        "post_delay": 1000,
        "next": [
            "回到主菜单"
        ]
    }
}